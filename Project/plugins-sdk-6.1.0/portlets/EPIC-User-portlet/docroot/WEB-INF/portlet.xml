<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>EPIC-User</portlet-name>
		<display-name>EPIC-User</display-name>
		<portlet-class>com.liferay.geoipusersmap.portlet.GeoIPUsersMapPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			 <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
		</supports>
		<portlet-info>		
			<title>EPIC-User</title>
			<short-title>EPIC-User</short-title>
			<keywords>EPIC-User</keywords>
		</portlet-info>
		<portlet-preferences>
            <preference>
                <name>country</name>
                <value>United Arab Emirates</value>
            </preference>
        </portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		
	</portlet>
</portlet-app>